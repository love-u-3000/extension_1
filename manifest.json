{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
   "action": {
  	"default_popup": "popup.html",
  	"default_icon": {
      "16": "downloads/images/get_started16.png",
      "32": "downloads/images/get_started32.png",
      "48": "downloads/images/get_started48.png",
      "128": "downloads/images/get_started128.png"
    }
  },
  "icons": {
    "16": "downloads/images/get_started16.png",
      "32": "downloads/images/get_started32.png",
      "48": "downloads/images/get_started48.png",
      "128": "downloads/images/get_started128.png"
  }
}